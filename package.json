{
  "name": "rest-graphql",
  "version": "0.1.6",
  "description": "Middleware for Express to adapt REST requests to GraphQL queries",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "prepublish": "babel src -d dist && ./node_modules/.bin/flow gen-flow-files src/index.js > dist/index.js.flow"
  },
  "keywords": [
    "graphql",
    "rest",
    "express"
  ],
  "author": "Gaurav Kulkarni <gaurav@remindhq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remind101/rest-graphql.git"
  },
  "dependencies": {
    "express": "^4.15.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "flow-bin": "^0.44.0",
    "jest": "^19.0.2",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "src"
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  },
  "peerDependencies": {
    "request-promise": "*"
  }
}
